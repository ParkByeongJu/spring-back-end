package di.xml;

import di.pojo.Tire;

public class Car {
	
	private Tire tire;

	public Car() {
		System.out.println("Car() 생성자 호출...");
	}
	
	//생성자 주입
	public Car(Tire tire) {
		this.tire = tire;
	}
	
	//세터 주입
	public void setTire(Tire tire) {
		this.tire = tire;
		System.out.println("setter 주입...");
	}
	
	public void printTireBrand() {
		System.out.println("장착된 타이어 : " + tire.getBrand());
	}
	
}
